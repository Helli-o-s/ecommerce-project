        "body": {
          "name": "Mechanical Keyboard",
          "price": 79.99,
          "stock": 75
        },
        "matchingRules": {
          "$.body.name": {
            "match": "type"
          },
          "$.body.price": {
            "match": "type"
          },
          "$.body.stock": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}